{ 
  "example1":
  {
    "key": "example1",
    "header": "Example 1",
    "description": "Change styles by classes and define them in CSS file for this Enyo component inside example1.js file",
    "example-cssnaming-enyo":
    [
      "enyo.kind({",
      "  name: 'OB.UI.ListInvoicesLine',",
      "  kind: 'OB.UI.CheckboxButton',",
      "  style: 'height: 58px; width: 100%; border-bottom: 1px solid #cccccc;',",
      "  components: [",
      "    {",
      "      name: 'line',",
      "      style: 'line-height: 23px; display: inline;',",
      "      components: [",
      "        {",
      "          name: 'topLine',",
      "          style: 'display: inline;'",
      "        },",
      "        {",
      "          name: 'bottonLine',",
      "          style: 'color: #888888;'",
      "        },",
      "        {",
      "          name: 'footerLine',",
      "          style: 'clear: both;'",
      "        }",
      "      ]",
      "    }",
      "  ]",
      "});"
    ],
    "example-cssnaming-css":
    [
      "/***********************   ***********************/",
      "/*                     File:                     */",
      "/*        Folder: src/cssNaming/examples/        */",
      "/***********************   ***********************/"
    ],  
    "result-cssnaming-enyo":
    [
      "enyo.kind({",
      "  name: 'OB.UI.ListInvoicesLine',",
      "  kind: 'OB.UI.CheckboxButton',",
      "  classes: 'obUiListInvoicesLine',",
      "  components: [",
      "    {",
      "      name: 'line',",
      "      classes: 'obUiListInvoicesLine-line',",
      "      components: [",
      "        {",
      "          name: 'topLine',",
      "          classes: 'obUiListInvoicesLine-line-topLine'",
      "        },",
      "        {",
      "          name: 'bottonLine',",
      "          classes: 'obUiListInvoicesLine-line-bottonLine'",
      "        },",
      "        {",
      "          name: 'footerLine',",
      "          classes: 'obUiListInvoicesLine-line-footerLine'",
      "        }",
      "      ]",
      "    }",
      "  ]",
      "});"
    ],
    "result-cssnaming-css":
    [
      "/***********************   ***********************/",
      "/*                File: example1.js              */",
      "/*        Folder: src/cssNaming/examples/        */",
      "/***********************   ***********************/",
      "/*",
      "# Component: OB.UI.ListInvoicesLine",
      "# Inherits: [OB.UI.CheckboxButton]",
      "*/",
      ".obUiListInvoicesLine {",
      "  height: 58px;",
      "  width: 100%;",
      "  border-bottom: 1px solid #cccccc;",
      "}",
      "",
      ".obUiListInvoicesLine-line {",
      "  line-height: 23px;",
      "  display: inline;",
      "}",
      "",
      ".obUiListInvoicesLine-line-topLine {",
      "  display: inline;",
      "}",
      "",
      ".obUiListInvoicesLine-line-bottonLine {",
      "  color: #888888;",
      "}",
      "",
      ".obUiListInvoicesLine-line-footerLine {",
      "  clear: both;",
      "}"
    ]
  }
}
